<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="listCellAnchorPane" prefHeight="90.0" prefWidth="354.0" style="-fx-background-color: #546e7a;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="taskImageView" fitHeight="49.0" fitWidth="51.0" layoutX="14.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/outline_task_white.png" />
         </image>
      </ImageView>
      <Label fx:id="taskTitleLabel" layoutX="83.0" layoutY="5.0" prefHeight="29.0" prefWidth="222.0" text="Title" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <Label fx:id="taskDescriptionLabel" layoutX="83.0" layoutY="34.0" prefHeight="29.0" prefWidth="222.0" text="Description" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="dateCreatedLabel" layoutX="8.0" layoutY="69.0" prefHeight="20.0" prefWidth="346.0" text="Date Created" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ImageView fx:id="deleteImageView" fitHeight="38.0" fitWidth="38.0" layoutX="313.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/outline_delete_white.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
